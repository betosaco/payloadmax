{
  "name": "nuo-lat",
  "version": "1.0.0",
  "description": "Website template for NUO",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "ci": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && pnpm build",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "dev:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next dev --turbo",
    "build:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next build",
    "start:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" next start",
    "payload:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-deprecation\" payload",
    "seed": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node --experimental-specifier-resolution=node src/seed/seed-script.js",
    "seed:payload": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation payload run src/seed/payload-seed.js",
    "seed:simple": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation payload run src/seed/simple-seed.js",
    "migrate:mcp": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/mcp-migration.js",
    "migrate:mcp-cursor": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/mcp-cursor-migration.js",
    "test:mcp-query": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/test-mcp-query.js",
    "check:connection": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/check-connection.js",
    "check:db": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/db-check.js",
    "check:collections": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation node src/seed/check-collections.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@payloadcms/db-vercel-postgres": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/plugin-cloud-storage": "^3.27.0",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-s3": "^3.27.0",
    "@payloadcms/storage-vercel-blob": "^3.27.0",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "15.2.0",
    "next-sitemap": "^4.2.3",
    "payload": "latest",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "s3": "link:@payloadcms/plugin-cloud-storage/s3",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@swc-node/register": "^1.10.9",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "^10.4.19",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.0",
    "http-browserify": "^1.7.0",
    "https-browserify": "^1.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
